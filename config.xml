<?xml version='1.0' encoding='UTF-8'?>
<widget xmlns="http://wirecloud.conwet.fi.upm.es/ns/macdescription/1" vendor="aui" name="mainWidgetV2" version="0.2.1">
    <details>
        <title>mainWidgetV2</title>
        <homepage>https://github.com/wirecloud-fiware/quick-start-development-tutorial</homepage>
        <authors>Anssi Uistola, Pekka Okkonen</authors>
        <contributors>noepe</contributors>
        <email>auistola@univ.yo.oulu.fi</email>
        <image>images/tutorialpic.jpg</image> <!-- 170x80 -->
        <description>Creates copy-paste curl commands based on searches</description>
        <longdescription>README.md</longdescription>
        <license>Apache License 2.0</license>
        <licenseurl>http://www.apache.org/licenses/LICENSE-2.0.html</licenseurl>
        <doc>doc/developer-guide.md</doc>
