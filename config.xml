    <!-- Rikkoo Wirecloudin mashupin :D
    <requirements>
        <feature name="NGSI"/>
    </requirements> -->
    <contents src="index.html" useplatformstyle="true"/>
	<rendering width="240" height="50%"/>

    <wiring>
        <inputendpoint
                name="recStartObject"
                label="recStartObject"
                type="text"/>
        <inputendpoint
                name="recSearchInfo"
                label="recSearchInfo"
                type="text"/>

        <outputendpoint
                name="sendCurl"
                label="Creates curls"
                type="text"/>
        <outputendpoint
                name="Graph"
                label="Graph"
                type="text"/>
        <outputendpoint
                name="sendCSV"
                label="CSV"
                type="text"/>
    </wiring>
</widget>
